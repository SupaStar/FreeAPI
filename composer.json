{
  "name": "alfredoobed/freeapi",
  "description": "Mini Framework php para creacion de API",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "firebase/php-jwt": "^5.3",
    "vlucas/phpdotenv": "^5.3",
    "ext-mysqli": "*",
    "rakit/validation": "^1.4",
    "illuminate/database": "8.48.1",
    "nesbot/carbon": "^2.49",
    "symfony/console": "^5.3",
    "illuminate/http": "*",
    "illuminate/container": "*",
    "illuminate/events": "*",
    "illuminate/routing": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Controllers\\": "app/controllers/",
      "Models\\": "app/models/"
    },
    "classmap": [
      "app/Console",
      "app/Console/Commands"
    ]
  },
